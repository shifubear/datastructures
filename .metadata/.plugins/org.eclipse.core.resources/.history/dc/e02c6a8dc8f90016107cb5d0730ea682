#ifndef LINKEDLIST_H_
#define LINKEDLIST_H_

template <class DataType>
class LinkedList {

public:
	LinkedList();
	~LinkedList();

private:
	struct SLLNode {
		DataType data;
		SLLNode* next;
	};

	SLLNode* head;
	unsigned size;

	friend class LinkedListTester;
};

#endif /* LINKEDLIST_H_ */
